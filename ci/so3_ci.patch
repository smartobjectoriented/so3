+static void set_y_anim(void * obj, int32_t v)
+{
+    lv_obj_set_y(obj, v);
+}
+
+static void set_width_anim(void * obj, int32_t v)
+{
+    lv_obj_set_width(obj, v);
+}
+
+static void arc_set_end_angle_anim(void * obj, int32_t v)
+{
+    lv_arc_set_end_angle(obj, v);
+}
