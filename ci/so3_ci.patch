+            lv_slider_set_value(obj, 5000, LV_ANIM_ON);    /*Animate to out of range value*/
+            auto_del(obj, LV_DEMO_STRESS_TIME_STEP * 5 + 22);
+
+            obj = lv_switch_create(main_page);
+
+            obj = lv_switch_create(main_page);
+            lv_obj_add_state(obj, LV_STATE_CHECKED);
+            auto_del(obj, 730);
+            break;
