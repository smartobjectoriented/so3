+/*
+ * demo.c
+ *
+ * A more complete LittlevGL demo.
+ *
+ * Based on:
+ *  - https://github.com/littlevgl/lv_examples/blob/master/lv_tests/lv_test_theme/lv_test_theme_1.c
+ */
+
+#include <stdio.h>
+#include <stdlib.h>
+#include <unistd.h>
+#include <fcntl.h>
+#include <sys/mman.h>
+#include <bits/ioctl_fix.h>
